<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="RedisSection" type="NFramework.TaskHandler.Redis.Configurations.RedisSection, NFramework.TaskHandler.Redis"/>
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <RedisSection>
    <RedisDBs>
      <RedisDB Name="localhost" ConnectionString="127.0.0.1:6379"></RedisDB>
    </RedisDBs>
    <RedisApps>
      <RedisApp Name="NF_TaskHandler" RedisDB="localhost"></RedisApp>
    </RedisApps>
  </RedisSection>
</configuration>